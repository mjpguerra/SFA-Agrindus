        <activity android:name="com.mobilefast.sfandroid.gui.GUIListaRoteiro" />
        <activity android:name="com.mobilefast.sfandroid.gui.GUIRotasCliente" />
        <activity android:name="com.mobilefast.sfandroid.gui.GUIFastSync" />
        <activity android:name="com.mobilefast.sfandroid.gui.GUIPrincipalFastSync" />
        <activity android:name="com.mobilefast.sfandroid.gui.GUIListaStatusPedido" />
