            android:windowSoftInputMode="stateAlwaysHidden" />
        <activity android:name="com.mobilefast.sfandroid.gui.GUICadastroCliente" />
        <activity
            android:name="com.mobilefast.sfandroid.gui.GUITabInfoCliente"
            android:windowSoftInputMode="stateAlwaysHidden" />
        <activity android:name="com.mobilefast.sfandroid.gui.GUIDetalheCliente" />
        <activity android:name="com.mobilefast.sfandroid.gui.GUIListaPedido" />
        <activity android:name="com.mobilefast.sfandroid.gui.GUIConsumoCliente" />
